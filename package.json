{
  "name": "c-w.github.io",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "static -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}'",
    "build": "html-inline -i index.html -o build.html --ignore-images --ignore-links",
    "lint.html": "htmllint index.html",
    "lint.js": "eslint *.js",
    "lint.css": "stylelint *.css",
    "lint": "npm-run-all lint.html lint.js lint.css"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-html": "^5.0.0",
    "html-inline": "^1.2.0",
    "htmllint-cli": "0.0.7",
    "node-static": "^0.7.11",
    "npm-run-all": "^4.1.5",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
